{
  "name": "micro-business-parse-server-backend",
  "version": "0.1.55",
  "description": "Micro Business Parse Server Backend",
  "main": "./dist/index.js",
  "scripts": {
    "start": "babel-node src/index.js",
    "build": "babel src --out-dir dist",
    "lint": "eslint --ext .jsx --ext .js src",
    "fixlint": "eslint --fix --ext .jsx --ext .js src",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test-verbose": "jest --verbose",
    "test-verbose:watch": "npm run test-verbose -- --watch",
    "publish-package": "npm run build && npm publish",
    "flow": "flow",
    "lint:staged": "lint-staged"
  },
  "author": "Micro Business ltd<micro.business.ltd@gmail.com>",
  "homepage": "https://bitbucket.org/microbusiness/micro-business-parse-server-backend#readme",
  "dependencies": {
    "express": "^4.15.4",
    "immutable": "^3.8.1",
    "parse-dashboard": "^1.1.0",
    "parse-server": "2.5.3",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "^7.2.1",
    "flow-bin": "^0.53.1",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.3",
    "prettier": "^1.5.3",
    "uuid": "^3.1.0"
  },
  "jest": {
    "rootDir": "./src"
  },
  "pre-commit": "lint:staged",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/microbusiness/micro-business-parse-server-backend.git"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote true --trailing-comma all --print-width 150",
      "git add"
    ]
  }
}
